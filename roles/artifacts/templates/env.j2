DATABASE_HOST="{{ database_host | default('localhost') }}"
DATABASE_USER="{{ database_user | default('postgres') }}"
DATABASE_PASSWORD="{{ database_password | default('') }}"
DATABASE_PORT="{{ database_port | default('5432') }}"
DATABASE_NAME="{{ database_name | default('postgres') }}"
DEBUG="{{ is_debug | default('True') }}"
REDIS_URL="{{ redis_url | default('redis://localhost:6379/0') }}"
DJANGO_SETTINGS_MODULE="{{ django_settings_module | default('appdj.settings.dev') }}"
DOCKER_HOST="{{ docker_host | default('tcp://127.0.0.1:2375') }}"
RABBITMQ_URL="{{ rabbitmq_url | default('amqp://@localhost/epsbekbl') }}"
AWS_STORAGE_BUCKET_NAME="{{ aws_storage_bucket_name | default('mysecretpassword') }}"
