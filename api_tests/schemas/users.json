{
  "profiles/": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "profile": {
          "type": "object",
          "properties": {
            "bio": {
              "type": ["null", "string"]
            },
            "url": {
              "type": ["null", "string"]
            },
            "location": {
              "type": ["null", "string"]
            },
            "company": {
              "type": ["null", "string"]
            },
            "timezone": {
              "type": ["null", "string"]
            },
            "avatar": {
              "type": ["null", "string"]
            }
          }
        }
      }
    }
  },
  "api-key/": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string"
      }
    }
  },
  "ssh-key/": {
    "type": "object",
    "key": {
      "type": "string"
    }
  }
}
