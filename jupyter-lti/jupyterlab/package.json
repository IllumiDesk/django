{
  "name": "@illumidesk/jupyter-lti",
  "version": "1.4.0",
  "description": "JupyterLab LTI extension for the Canvas LMS.",
  "keywords": [
    "lti",
    "canvas lms",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/illumidesk/illumidesk/jupyter-lti",
  "bugs": {
    "url": "https://github.com/illumidesk/illumidesk/issues"
  },
  "license": "BSD-3-Clause",
  "author": "IllumiDesk Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.*"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/illumidesk/illumidesk.git"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.1.2",
    "@jupyterlab/apputils": "^1.1.2",
    "@jupyterlab/notebook": "^1.1.2",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/messaging": "^1.3.0",
    "@phosphor/widgets": "^1.9.1",
    "moment": "^2.23.0"
  },
  "devDependencies": {
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "style-loader": "1.0.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.3",
    "webpack": "4.40.1"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jupyterlab": {
    "extension": true
  }
}
